{
  "name": "webpack-4-template",
  "version": "1.0.0",
  "description": "Template set up with Webpack 4, Sass, Postcss",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --open --config webpack/dev.config.js",
    "sass": "node-sass -w src/scss/ -o src/ --recursive",
    "build": "NODE_ENV=production webpack --config webpack/prod.config.js"
  },
  "author": "Pamela Hicks <pamelasusanhicks@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^8.0.2",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.4.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0"
  }
}
